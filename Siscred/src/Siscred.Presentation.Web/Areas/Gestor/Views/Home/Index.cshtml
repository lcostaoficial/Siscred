@{
    ViewBag.Title = "Página Inicial";   
}

@Html.Partial("_Title")

<div class="row panel-position">    
    <div class="col-lg-12">
        <div class="panel panel-primary">
            <div class="panel-heading">
                <h3 class="panel-title">Estatísticas</h3>
            </div>
            <div class="panel-body">
                <div class="list-group">
                    <a href="#" class="list-group-item">
                        <h4 class="list-group-item-heading">Total de Usuários</h4>
                        <p class="list-group-item-text">@ViewBag.QtdeUsuario</p>
                    </a>
                    <a href="#" class="list-group-item">
                        <h4 class="list-group-item-heading">Total de Inscrições</h4>
                        <p class="list-group-item-text">@ViewBag.QtdeInscricao</p>
                    </a>
                </div>
            </div>
        </div>
    </div>
</div>